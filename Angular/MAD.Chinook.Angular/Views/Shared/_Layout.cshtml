<!DOCTYPE html>
<html ng-app="app">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Cibertec.Angular</title>
    <environment names="Development">
        <link href="~/libraries/bootstrap/css/bootstrap.css" rel="stylesheet" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment names="Production">
        <link href="~/libraries/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>

</head>
<body ng-controller="applicationController as vm"
      ng-init="init('@ViewBag.WebApiUrl')">
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a ui-sref="home" ui-sref-active="active" class="navbar-brand">Cibertec.Angular</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a ng-if="!vm.validate()" ui-sref="login" ui-sref-active="active">Login</a></li>
                    <li><a ng-if="vm.validate()" ng-click="vm.logout()" href="#" ui-sref-active="active">Logout</a></li>
                    <li><a ng-if="vm.validate()" ui-sref="customer" ui-sref-active="active">Customer</a></li>
                    <li><a ng-if="vm.validate()" ui-sref="artist" ui-sref-active="active">Artist</a></li>
                    <li><a ng-if="vm.validate()" ui-sref="employee" ui-sref-active="active">Employee</a></li>
                    <li><a ng-if="vm.validate()" ui-sref="invoice" ui-sref-active="active">Invoice</a></li>
                    <li><a ng-if="vm.validate()" ui-sref="invoiceLine" ui-sref-active="active">InvoiceLine</a></li>
                    <li><a ng-if="vm.validate()" ui-sref="mediaType" ui-sref-active="active">MediaType</a></li>
                    <li><a ng-if="vm.validate()" ui-sref="playlist" ui-sref-active="active">Playlist</a></li>
                    <li><a ng-if="vm.validate()" ui-sref="genre" ui-sref-active="active">Genre</a></li>
                    <li><a ng-if="vm.validate()" ui-sref="album" ui-sref-active="active">Album</a></li>
                    <li><a ng-if="vm.validate()" ui-sref="track" ui-sref-active="active">Track</a></li>

                </ul>
            </div>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2017 - Cibertec.Angular</p>
        </footer>
    </div>

    <environment names="Development">

        <script src="~/libraries/jquery/jquery.js"></script>
        <script src="~/libraries/bootstrap/js/bootstrap.js"></script>

        <script src="~/libraries/angular/angular.js"></script>
        <script src="~/libraries/angular/angular-ui-router.js"></script>
        <script src="~/libraries/angular/angular-local-storage.js"></script>
        <script src="~/libraries/angular/ui-bootstrap-tpls.js"></script>

        <script src="~/app/app.js"></script>
        <script src="~/app/app.config.js"></script>
        <script src="~/app/app.controller.js"></script>
        <script src="~/app/app.routes.js"></script>
        <script src="~/app/app.interceptor.js"></script>

        <script src="~/app/shared/data.service.js"></script>
        <script src="~/app/shared/settings.service.js"></script>
        <script src="~/app/shared/authentication.service.js"></script>



        <script src="~/app/public/login/login.controller.js"></script>

        <script src="~/app/components/modal/modal.directive.js"></script>

        
        <script src="~/app/private/customer/directives/customer-card/customer.card.js"></script>
        <script src="~/app/private/customer/directives/customer-form/customer.form.js"></script>
        <script src="~/app/private/customer/customer.controller.js"></script>

        <script src="~/app/private/artist/directives/artist-card/artist.card.js"></script>
        <script src="~/app/private/artist/directives/artist-form/artist.form.js"></script>
        <script src="~/app/private/artist/artist.controller.js"></script>

        <script src="~/app/private/employee/directives/employee-card/employee.card.js"></script>
        <script src="~/app/private/employee/directives/employee-form/employee.form.js"></script>
        <script src="~/app/private/employee/employee.controller.js"></script>

        <script src="~/app/private/invoice/directives/invoice-card/invoice.card.js"></script>
        <script src="~/app/private/invoice/directives/invoice-form/invoice.form.js"></script>
        <script src="~/app/private/invoice/invoice.controller.js"></script>


        <script src="~/app/private/invoiceLine/directives/invoice-line-card/invoice.line.card.js"></script>
        <script src="~/app/private/invoiceLine/directives/invoice-line-form/invoice.line.form.js"></script>
        <script src="~/app/private/invoiceLine/invoice.line.controller.js"></script>

        <script src="~/app/private/mediaType/directives/media-type-card/media.type.card.js"></script>
        <script src="~/app/private/mediaType/directives/media-type-form/media.type.form.js"></script>
        <script src="~/app/private/mediaType/media.type.controller.js"></script>

        
        <script src="~/app/private/playlist/directives/playlist-card/playlist.card.js"></script>
        <script src="~/app/private/playlist/directives/playlist-form/playlist.form.js"></script>
        <script src="~/app/private/playlist/playlist.controller.js"></script>


        <script src="~/app/private/genre/directives/genre-card/genre.card.js"></script>
        <script src="~/app/private/genre/directives/genre-form/genre.form.js"></script>
        <script src="~/app/private/genre/genre.controller.js"></script>

        <script src="~/app/private/album/directives/album-card/album.card.js"></script>
        <script src="~/app/private/album/directives/album-form/album.form.js"></script>
        <script src="~/app/private/album/album.controller.js"></script>

        <script src="~/app/private/track/directives/track-card/track.card.js"></script>
        <script src="~/app/private/track/directives/track-form/track.form.js"></script>
        <script src="~/app/private/track/track.controller.js"></script>


    </environment>

    <environment names="Production">

        <script src="~/js/bootstrap.min.js"></script>
        <script src="~/js/angular.min.js"></script>
        <script src="~/js/app-site.min.js"></script>
        <script src="~/js/app-cibertec.min.js"></script>


    </environment>
</body>
</html>
